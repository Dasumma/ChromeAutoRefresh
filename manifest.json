{
	"name": "Refresh Page",
	"description": "Refreshes Page On Interval",
	"version": "1.1",
	"background" : {
		"service_worker" : "js/service-worker.js"
	},
	"manifest_version": 3,
	"action": {
		"default_popup": "html/RefreshPage.html",
		"default_icon": "media/favicon.png"
	},
	"permissions": [
		"storage"
	]
}
